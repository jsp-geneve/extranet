{
  "name": "jsp-extranet",
  "version": "0.0.1",
  "description": "A Quasar Framework app",
  "productName": "Extranet JSP Gen√®ve",
  "cordovaId": "org.cordova.quasar.app",
  "author": "Franky Snow <FrankySnow@users.noreply.github.com>",
  "private": true,
  "scripts": {
    "dev": "quasar dev",
    "lint": "eslint --ext .js,.vue src",
    "test": "echo \"See package.json => scripts for available tests.\" && exit 0",
    "test:unit": "jest --notify --updateSnapshot",
    "test:unit:coverage": "jest --notify --coverage",
    "test:unit:watch": "jest --notify --watch",
    "test:unit:watchAll": "jest --notify --watchAll",
    "serve:test:coverage": "quasar serve test/jest/coverage/lcov-report/ --port 8788",
    "concurrently:dev:jest": "concurrently \"quasar dev\" \"jest --notify --watch\"",
    "test:e2e": "cypress open",
    "test:e2e:CI": "cypress run"
  },
  "dependencies": {
    "@quasar/extras": "^1.1.2",
    "apollo-cache-inmemory": "^1.5.1",
    "apollo-client": "^2.5.1",
    "apollo-link": "^1.2.11",
    "apollo-link-context": "^1.0.18",
    "apollo-link-http": "^1.5.14",
    "eslint-plugin-cypress": "^2.2.1",
    "graphql": "^14.2.1",
    "graphql-tag": "^2.10.1",
    "quasar": "^1.0.0-rc.4",
    "vue-apollo": "^3.0.0-beta.28"
  },
  "devDependencies": {
    "@quasar/app": "^1.0.0-rc.6",
    "@quasar/quasar-app-extension-testing": "^1.0.0-rc.9",
    "@quasar/quasar-app-extension-testing-e2e-cypress": "^1.0.0-beta.10",
    "@quasar/quasar-app-extension-testing-unit-jest": "^1.0.0-beta.24",
    "@vue/eslint-config-standard": "^4.0.0",
    "babel-eslint": "^10.0.1",
    "eslint": "^5.10.0",
    "eslint-loader": "^2.1.1",
    "eslint-plugin-vue": "^5.0.0",
    "strip-ansi": "=3.0.1"
  },
  "engines": {
    "node": ">= 8.9.0",
    "npm": ">= 5.6.0",
    "yarn": ">= 1.6.0"
  },
  "browserslist": [
    "last 1 version, not dead, ie >= 11"
  ],
  "resolutions": {
    "ajv": "6.8.1"
  }
}
